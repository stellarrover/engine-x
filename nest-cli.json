{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "monorepo": true,
  "root": ".",
  "compilerOptions": {},
  "projects": {
    "process": {
      "type": "application",
      "root": ".",
      "entryFile": "/modules/process/process.main",
      "sourceRoot": "./src",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.json"
      }
    },
    "workflow": {
      "type": "application",
      "root": ".",
      "entryFile": "/modules/workflow/workflow.main",
      "sourceRoot": "./src",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.json"
      }
    },
    "": {
      "type": "application",
      "root": ".",
      "entryFile": "main",
      "sourceRoot": "./src",
      "compilerOptions": {
        "tsConfigPath": "tsconfig.json"
      }
    }
  }
}
